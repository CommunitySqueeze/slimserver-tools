<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Root>
[%- FOR file IN data.files %]
	[%- IF data.usefile.$file.$target %]
	<File path="[% file %]">
	[%- FOR string IN data.data.$file.keys %]
		<TranslationUnit Id = "[% string %]">
			<Source><![CDATA[[% data.data.$file.$string.EN %]]]></Source>
			<Target>[% IF data.data.$file.$string.$target %]<![CDATA[[% data.data.$file.$string.$target %]]]>[% END %]</Target>
		</TranslationUnit>
	[%- END %]
	</File>
	[%- END %]
[%- END %]
</Root>
